{
  "name": "Medio Plugin",
  "description": "A plugin to help you join a Medio Virtual Meeting Room.",
  "icons":{
    "128": "icon128.png",
    "48": "icon48.png",
    "16": "icon16.png"
},
  "version": "1.0",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "browser_action": {
      "default_title": "Medio Plugin",
      "default_icon": "icon16.png",
      "default_popup": "popup.html"
  },

  "manifest_version": 2
}
